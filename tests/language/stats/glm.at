9  -1  1  0  0 -1  0  0 2.00
3  -1  0  1  0  0 -1  0 2.00
8  -1  0  1  0  0 -1  0 2.00
9  -1  0  1  0  0 -1  0 2.00
11 -1  0  1  0  0 -1  0 2.00
9  -1  0  0  1  0  0 -1 2.00
12 -1  0  0  1  0  0 -1 2.00
14 -1  0  0  1  0  0 -1 2.00
