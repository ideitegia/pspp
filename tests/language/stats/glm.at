end data.

do if B0 = -1 AND B1 = -1 AND B2 = -1.
compute Bgrp = 4.
end if.

do if B0 = 0 AND B1 = 0 AND B2 = 1.
compute Bgrp = 3.
end if.

do if B0 = 0 AND B1 = 1 AND B2 = 0.
compute Bgrp = 2.
end if.

do if B0 = 1 AND B1 = 0 AND B2 = 0.
compute Bgrp = 1.
end if.
])

AT_DATA([type1.sps], [dnl
include 'data-inc.sps'.

glm dv by Agrp Bgrp
	/method = sstype (1)
	.

glm dv by Agrp Bgrp
	/method = sstype (1)
	/design Bgrp Agrp Bgrp * Agrp
	.
])


