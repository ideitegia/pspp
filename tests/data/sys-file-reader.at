2; -1; 0; 0; 0; 0; s8 "";

dnl Data.
999; 0;
s14 "one data item";
s8 "partial";
])
for variant in \
	"be 4a9e84f9e679afb7bb71acd0bb7eab89" \
	"le 30752606f14ee2deec2854e8e6de4b3b"
do
  set $variant
  AT_CHECK_UNQUOTED([sack --$[1] sys-file.sack > sys-file.sav], [0], [], [$[2]
